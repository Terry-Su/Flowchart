{
	"include": [
		"src/**/*",
		"../Draw/src/typings",
	],
	"exclude": [
		"node_modules",
		"src/__tmp__"
	],
	"compilerOptions": {
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"outDir": "./build/",
		"sourceMap": true,
		"lib": [
			"dom",
			"es2016"
		],
		"typeRoots": [
			"src/typings",
			"../Draw/src/typings",
		]
	}
}
